{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "lab1 gdb launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/lab1/build/Lab1",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
          {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          },
          {
              "description": "Set Disassembly Flavor to Intel",
              "text": "-gdb-set disassembly-flavor intel",
              "ignoreFailures": true
          }
      ],
      "preLaunchTask": "lab1 build with CMake"
    }
  ]
}